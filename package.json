		"@types/jquery": "^3.5.5",
		"@types/mocha": "^8.2.0",
		"@types/sinon": "^9.0.10",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"babel-loader": "^8.2.2",
		"chai": "^4.3.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^6.1.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"esm": "^3.2.25",
		"jsdom": "16.4.0",
		"jsdom-global": "3.0.2",
		"mocha": "^8.2.1",
		"prettier": "^1.18.2",
		"sinon": "^2.4.1",
		"sinon-chai": "^2.12.0",
		"terser-webpack-plugin": "^5.1.1",
		"ts-loader": "^6.2.2",
		"ts-node": "^9.1.1",
		"typescript": "^3.9.9",
		"webpack": "^5.21.2",
		"webpack-cli": "^3.3.12"
	},
