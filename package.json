		"@types/jquery": "^3.3.31",
		"@types/mocha": "^8.0.4",
		"@types/sinon": "^9.0.9",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"babel-loader": "^8.0.6",
		"chai": "^4.0.2",
		"eslint": "^6.1.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.1.0",
		"esm": "^3.2.25",
		"jsdom": "16.4.0",
		"jsdom-global": "3.0.2",
		"mocha": "^8.2.1",
		"prettier": "^1.18.2",
		"sinon": "^2.4.1",
		"sinon-chai": "^2.12.0",
		"terser-webpack-plugin": "^5.0.3",
		"ts-loader": "^6.0.4",
		"ts-node": "^9.0.0",
		"typescript": "^3.5.3",
		"webpack": "^5.9.0",
		"webpack-cli": "^3.3.11"
	},
