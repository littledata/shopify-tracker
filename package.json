{
	"scripts": {
		"build-master": "node ./node_modules/webpack/bin/webpack.js",
		"build-staging": "node ./node_modules/webpack/bin/webpack.js --mode none",
		"start": "node ./node_modules/webpack/bin/webpack.js --watch",
		"lint": "node ./node_modules/eslint/bin/eslint.js --ext .ts ./src",
		"prettier": "prettier --write \"**/*.{[tj]s,html}\"",
		"test": "env TEST=true mocha -r esm -r ts-node/register --recursive --parallel  ./test/**/**/**/**/*.ts",
		"test-watch": "env TEST=true mocha -w -r esm -r ts-node/register --recursive  ./test/**/**/**/**/*.ts"
	},
	"devDependencies": {
		"@babel/core": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@types/chai": "^4.2.15",
